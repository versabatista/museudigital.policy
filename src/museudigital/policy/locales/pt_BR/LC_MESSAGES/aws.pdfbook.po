#: aws.pdfbook/aws/pdfbook/interfaces.py:19
msgid "label_disallowed_types"
msgstr "Tipos de conteúdos descartados"

#. Default: "\"htmldoc\" executable"
#: aws.pdfbook/aws/pdfbook/interfaces.py:37
msgid "label_htmldoc_path"
msgstr "Executável \"htmldoc\""

#. Default: "Render PDF in the browser"
#: aws.pdfbook/aws/pdfbook/interfaces.py:75
msgid "label_inline_render"
msgstr "Renderizar o PDF no navegador"

#. Default: "\"recode\" executable"
#: aws.pdfbook/aws/pdfbook/interfaces.py:25
msgid "label_recode_path"
msgstr "Executável \"recode\""

#. Default: "View a printable page"
#: aws.pdfbook/aws/pdfbook/browser/contentform.py:54
